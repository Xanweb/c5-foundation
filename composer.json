{
    "name": "xanweb/c5-foundation",
    "description": "Concrete5 Foundation",
    "keywords": ["concrete5", "concrete5-v8", "library"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/polyfill-php80": "^1.20",
        "xanweb/c5-common": "^1.1",
        "xanweb/c5-entity": "^1.0",
        "xanweb/c5-request": "^1.0",
        "xanweb/c5-helpers": "^1.2",
        "xanweb/c5-js-localization": "^1.0",
        "xanweb/c5-block": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Xanweb\\Foundation\\": ""
        }
    },
    "extra" : {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        },
        "concrete5" : {
            "providers" : [
                "Xanweb\\Foundation\\ServiceProvider"
            ]
        }
    },
    "support": {
        "source": "https://github.com/Xanweb/c5-foundation"
    }
}

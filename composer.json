{
    "name": "xanweb/c5-foundation",
    "description": "ConcreteCMS Foundation",
    "keywords": ["ConcreteCMS", "concrete5", "concrete5-v9", "ConcreteCMS-v9", "library"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "symfony/polyfill-php80": "^1.20",
        "xanweb/c5-common": "^1.2",
        "xanweb/c5-request": "^1.2",
        "xanweb/c5-helpers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Xanweb\\C5\\Foundation\\": ""
        }
    },
    "extra" : {
        "branch-alias": {
            "dev-develop": "2.x-dev"
        },
        "concrete5" : {
            "providers" : [
                "Xanweb\\C5\\Foundation\\ServiceProvider"
            ]
        }
    },
    "support": {
        "source": "https://github.com/Xanweb/c5-foundation"
    }
}

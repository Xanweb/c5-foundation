{
    "name": "xanweb/c5-foundation",
    "description": "Concrete5 Foundation",
    "keywords": ["concrete5", "concrete5-v8", "library"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/polyfill-php80": "^1.20",
        "xanweb/c5-helpers": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Xanweb\\Foundation\\": "src/",
            "Xanweb\\Foundation\\Controller\\": "controllers/"
        }
    },
    "extra" : {
        "concrete5" : {
            "providers" : [
                "Xanweb\\Foundation\\ServiceProvider"
            ]
        }
    },
    "support": {
        "source": "https://github.com/Xanweb/c5-foundation"
    }
}
